<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Avoid_Car_Empty</fullName>
    <active>true</active>
    <description>This rule prevents saving a Warranty without an associated Car.</description>
    <errorConditionFormula>ISBLANK(Car__c)</errorConditionFormula>
    <errorDisplayField>Car__c</errorDisplayField>
    <errorMessage>Car field can&apos;t be empty</errorMessage>
</ValidationRule>
